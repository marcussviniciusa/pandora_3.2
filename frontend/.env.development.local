# Configurações de desenvolvimento local
# Este arquivo é específico para o ambiente local e não deve ser versionado

# Defina como true para utilizar a API real em desenvolvimento
# VITE_USE_REAL_API=true

# Configuração de depuração para o Mock Service Worker
VITE_MSW_DEBUG=true

# Atraso artificial para simular latência de rede (em ms)
VITE_MOCK_DELAY=300

# Probabilidade de erro simulado (0-100)
VITE_MOCK_ERROR_RATE=5
